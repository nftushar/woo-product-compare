{
  "apiVersion": "1",
  "name": "b-blocks/product-compare",
  "title": "product-compare",
  "description": "Attractive product compare for WooCommerce",
  "category": "widgets",
  "keywords": [
    "product-compare",
    "WooCommerce",
    "product"
  ],
  "textdomain": "product-compare",
  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "cId": {
      "type": "string",
      "default": ""
    },
    "productIds": {
      "type": "array",
      "default": []
    },
    "border": {
      "type": "object",
      "default": {
        "radius": "5px",
        "color": "#000",
        "width": "1px"
      }
    },
    "alignment": {
      "type": "string",
      "default": "center"
    },
    "layout": {
      "type": "string",
      "default": "box_count"
    },
    "shareOffOn": {
      "type": "string",
      "default": "true"
    },
    "showFaces": {
      "type": "string",
      "default": "true"
    },
    "clrScheme": {
      "type": "string",
      "default": "dark"
    },
    "background": {
      "type": "object",
      "default": {
        "color": "#0000"
      }
    },
    "padding": {
      "type": "object",
      "default": {
        "top": "20px",
        "right": "20px",
        "bottom": "20px",
        "left": "20px"
      }
    },
    "btnStyle": {
      "type": "object",
      "default": {
        "colors": {
          "color": "#fff",
          "bg": "#525CEB"
        },
        "hvrColors": {
          "color": "#111",
          "bg": "#FF9800"
        },
        "border": {
          "radius": "5px",
          "color": "#333",
          "width": "1px"
        },
        "padding": {
          "top": "10px",
          "right": "20px",
          "bottom": "10px",
          "left": "20px"
        },
        "typography": {
          "fontSize": {
            "desktop": 16
          }
        }
      }
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "example": {
    "attributes": {
      "preview": true,
      "columns": {
        "desktop": 1,
        "tablet": 1,
        "mobile": 1
      }
    }
  },
  "editorScript": "file:../dist/editor.js"
}